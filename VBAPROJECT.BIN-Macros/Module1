Sub Dolch_Word_List()
    Worksheets("dolchwordlist").Range("E6:E6").Value = ""
    
    
    
    
    Dim vArray As Variant
    vArray = Sheets(1).Range("A1:LC1").Value
    Dim myLength As Integer
    If (varIsSet = 0) Then
        globalArray = vArray
        varIsSet = 1
    End If
    myLength = Application.CountA(globalArray)

    Dim randomNum As Integer
    Randomize
    'Int ((upperbound - lowerbound + 1) * Rnd + lowerbound)
    randomNum = Int((myLength - 0 + 1) * Rnd + 1)
    If (randomNum > myLength) Then
        randomNum = randomNum - 1
    End If
    If (randomNum = 0) Then
        randomNum = 1
    End If
    
    Dim word As String
    Dim count As Integer
     word = globalArray(1, randomNum)
    For count = 1 To myLength
       
        If (word = "0") Then
            randomNum = Int((myLength - 0 + 1) * Rnd + 0)
            If (randomNum > myLength) Then
                randomNum = randomNum - 1
            End If
            If (randomNum = 0) Then
                randomNum = 1
            End If
            word = globalArray(1, randomNum)
            Worksheets("dolchwordlist").Range("E6:E6").Value = "0"
        Else
             Worksheets("dolchwordlist").Range("E6:E6").Value = word
             globalArray(1, randomNum) = "0"
             Exit For
        End If
    Next
    If (Worksheets("dolchwordlist").Range("E6:E6").Value = "0") Then
         Worksheets("dolchwordlist").Range("E6:E6").Value = ""
        MsgBox ("Congratulations, you have finished the list. Click the Regenerate List button to start over")
    End If
End Sub
Sub Auto_Close()
    ThisWorkbook.Saved = True
End Sub

Public Sub Foobar()

Dim foo As String
 foo = 12
End Sub

